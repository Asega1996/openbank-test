{
  "presets": [
    "react-app",
    ["@babel/preset-env",
      {"targets": {"node": "current" }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ".",
        "alias": {
          "@Assets": "./src/assets",
          "@Constants": "./src/constants",
          "@Components": "./src/components",
          "@Screens": "./src/screens",
          "@Router": "./src/router",
          "@Services": "./src/services",
          "@Store": "./src/store",
          "@Styles": "./src/styles",
          "@Utils": "./src/utils"
        }
      }
    ]
  ]
}

